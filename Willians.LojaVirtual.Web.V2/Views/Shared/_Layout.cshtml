                        <a href="Login.html">Seja bem-vindo(a)!  Faça seu login </a>

                        <a href="login.html">Meu cadastro</a>
                        <a href="register.html">Meus pedidos</a>
                        <a data-toggle="modal" href="#shoppingcart"><i class="fa fa-shopping-cart"></i> 3 itens - R$ 300,00</a>
                    </div>
                </div>

            </div>

            <div class="row">

                <div class="col-md-12 col-sm-12">
                    @Html.Partial("_Menu")
                </div>


            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12">

                    @using (Html.BeginRouteForm("BuscaProduto", FormMethod.Get))
                    {
                        <div class="input-group">
                            @Html.TextBox("termo", null, new { @class = "form-control", placeholder = "Clique aqui e faça sua busca" })
                            <span class="input-group-btn">
                                <button type="submit" class="btn btn-danger">Ok</button>
                            </span>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>

